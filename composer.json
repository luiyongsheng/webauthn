{
	"authors": [
		{
			"email": "luiyongsheng@rentrar.com",
			"name": "Lui Yong Sheng"
		}
	],
	"autoload": {
		"psr-4": {
			"LuiYongSheng\\Webauthn\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"description": "Allow users to login with FaceID or TouchID in your Laravel app.",
	"extra": {
		"laravel": {
			"providers": [
				"LuiYongSheng\\WebAuthn\\WebAuthnServiceProvider"
			]
		}
	},
	"license": "MIT",
	"minimum-stability": "dev",
	"name": "luiyongsheng/webauthn",
	"require": {
		"illuminate/contracts": "^8.0",
		"illuminate/database": "^8.0",
		"illuminate/http": "^8.0",
		"illuminate/support": "^8.0",
		"illuminate/validation": "^8.0",
		"nyholm/psr7": "^1.3",
		"php": "^7.2.5|^8.0",
		"psr/http-message": "^1.0",
		"symfony/psr-http-message-bridge": "^2.0",
		"web-auth/webauthn-lib": "^3.3"
	},
	"scripts": {
		"cs": "php-cs-fixer fix . --config=.php_cs --allow-risky=yes",
		"psalm": "vendor/bin/psalm"
	}
}
